{
  "name": "@levino/shipyard-base",
  "version": "0.1.1",
  "type": "module",
  "main": "lib/index.js",
  "peerDependencies": {
    "astro": "^5",
    "daisyui": "^4",
    "tailwindcss": "^3",
    "@tailwindcss/typography": "^0.5.10"
